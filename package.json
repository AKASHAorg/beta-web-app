{
  "name": "web-app",
  "version": "1.0.0",
  "description": "AKASHA for web 2.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-lang": "babel ./app/utils/build-translations.js | node",
    "flow": "./node_modules/.bin/flow",
    "prestart": "tsc -p tsconfig.json && npm run build-lang",
    "start": "cross-env NODE_ENV=development node -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config ./webpack.config.development.js",
    "start-dark": "cross-env NODE_ENV=development DARK_THEME=1 node -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config ./webpack.config.development.js",
    "production": "tsc -p tsconfig.json && cross-env NODE_ENV=production node --max-old-space-size=4096 --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build-worker": "cross-env NODE_ENV=production node --max-old-space-size=4096 --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.worker.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node --max-old-space-size=5096 --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build-renderer-dark": "cross-env NODE_ENV=production DARK_THEME=1 node --max-old-space-size=5096 --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build": "tsc -p tsconfig.json && npm run build-renderer && npm run build-renderer-dark"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AkashaProject/web-app.git"
